{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "llm",
  "title": "LLM",
  "description": "Access different LLM and pass info to other applications",
  "icon": "extension_icon2.png",
  "author": "brunnorpdias",
  "categories": [
    "Productivity",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "chat_form",
      "title": "Quick Question",
      "description": "Sends the request to the server",
      "mode": "view"
    },
    {
      "name": "assistant_form",
      "title": "Assistant",
      "description": "Work on complex problems and files",
      "mode": "view"
    },
    {
      "name": "chat_bookmarks",
      "title": "View Bookmarks",
      "description": "View all bookmarked conversations",
      "mode": "view"
    }
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "@mistralai/mistralai": "^0.0.10",
    "@raycast/api": "^1.66.1",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "eventsource": "^2.0.2",
    "fetch": "^1.1.0",
    "groq-sdk": "^0.3.2",
    "llamaai": "^1.0.4",
    "node-fetch": "^3.3.2",
    "openai": "^4.26.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
